<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="context1">
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection connectionURL="jdbc:mysql://127.0.0.1:3306/snowberghobby"
			driverClass="com.mysql.jdbc.Driver" password="root" userId="root" />
		<javaModelGenerator targetPackage="com.snowhobby.db.entity"
			targetProject="SnowHobby/src/main/java">
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="com.snowhobby.db.mapper"
			targetProject="SnowHobby/src/main/resources" />
		<javaClientGenerator targetPackage="com.snowhobby.db.dao"
			targetProject="SnowHobby/src/main/java" type="XMLMAPPER" />



		<table schema="snowberghobby" tableName="t_agency" mapperName="AgencyDAO"
			domainObjectName="Angency" enableCountByExample="false"
			enableSelectByExample="false" enableDeleteByExample="false"
			enableUpdateByExample="false">
			<columnRenamingRule searchString="^t_"
				replaceString="" />
		</table>

		<table schema="snowberghobby" tableName="t_attenchment" mapperName="AttenchmentDAO"
